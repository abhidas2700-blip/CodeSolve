{
  "name": "thoreye-audit-management",
  "version": "1.0.0",
  "description": "ThorEye Quality Assurance Audit Management System",
  "main": "dist/server/production-clean.js",
  "scripts": {
    "start": "node dist/server/production-clean.js",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build --config vite.config.ts",
    "build:server": "tsc server/production-clean.ts --outDir dist/server --target es2020 --module commonjs --esModuleInterop --allowSyntheticDefaultImports --strict --skipLibCheck",
    "dev": "tsx server/index.ts",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.9.0",
    "bcrypt": "^5.1.1",
    "drizzle-orm": "^0.33.0",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.17.10",
    "@types/node": "^20.11.5",
    "@types/passport": "^1.0.16",
    "@types/passport-local": "^1.0.38",
    "@types/ws": "^8.5.10",
    "@vitejs/plugin-react": "^4.2.1",
    "drizzle-kit": "^0.24.0",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vite": "^5.0.12"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}